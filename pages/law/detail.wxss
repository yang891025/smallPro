/**index.wxss**/

.track-bg {
  background-color: #fff;
  margin-top: 30rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
  text-align: center;
  border-radius:2px;
  position: relative;
}
.weui-search-bar {
  position: relative;
  padding: 8px 10px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  box-sizing: border-box;
  background-color: #fff;
  border-top: 1rpx solid #fff;
  border-bottom: 1rpx solid #d7d6dc;
}
.radio-group {
  font-size: 15px;
  display: flex;
  flex-direction: column;
}
.checkbox-group {
  font-size: 16px;
  display: flex;
  flex-direction: column;
}
.page-text {
 text-align: center;
 font-size: 32px;
 margin-top: 60rpx;
 color: #1188ee;
}
.weui-flex {
  width: 100%;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}