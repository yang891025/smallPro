/* pages/fcm/fcm.wxss */
.home-wrap {
  background: #f2f2f2;
}

.workbench {
  font-size: 16px;
  background: #fff;
  padding-bottom: 10rpx;
  margin-bottom: 20rpx;
  color: #333;
}

.workbench .title {
  font-size: 18px;
  padding: 10rpx 10rpx;
  margin-bottom: 20rpx;
  display: block;
}

.workbench .list {
  width: 100%;
  /* display: flex; *//* flex-direction: row;
  flex:1; */
}

.workbench .items {
  width: 33%;
  text-align: center;
  display: inline-block;
  margin-bottom: 35rpx;
  /* width: 100rpx;
  flex:1;
  text-align: center; */
}

.workbench .items image {
  width: 120rpx;
  height: 120rpx;
  justify-content: center;
}

.workbench .items text {
  display: block;
  text-align: center;
  margin-top: 10rpx;
}